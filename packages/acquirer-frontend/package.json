{
  "name": "acquirer-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "ISC",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type:check": "tsc --noEmit",
    "dep:check": "npx ncu -e 2",
